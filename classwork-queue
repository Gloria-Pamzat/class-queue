class Queue:
    def __init__(self, name, age):
        self.name = name
        self.age = age
        
    def put_age (self):
        return self.age
        
    def get_name (self):
        return self.name
 
      animal_name = "dog"
      animal_age = 1
      

que = Queue(animal)
que.put(1)
que.put("dog")
que.put(False)
try:
    for i in range(4):
        print(que.get_name())
except:
    print("Queue error")
